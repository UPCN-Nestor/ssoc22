        <addForeignKeyConstraint baseColumnNames="plan_id"
                                 baseTableName="provision"
                                 constraintName="fk_provision__plan_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_plan"/>
    </changeSet>
</databaseChangeLog>
